<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:views="clr-namespace:DickHelper.Views"
             x:Class="DickHelper.Views.TabHostView">
    <TabControl>
        <TabItem Header="主页">
            <views:MainView />
        </TabItem>
        <TabItem Header="历史记录">
            <views:HistoryView />
        </TabItem>
        <TabItem Header="统计">
            <views:StatisticsView />
        </TabItem>
        <TabItem Header="AI">
            <views:AIAnalysisView />
        </TabItem>
        <TabItem Header="同步">
            <views:SyncView />
        </TabItem>
    </TabControl>
</UserControl>
